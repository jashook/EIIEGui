    <Grid>
        <Label Content="Press FINISH to begin experiment. You must NOT move your head after this time." HorizontalAlignment="Left" Margin="170,346,0,0" VerticalAlignment="Top" Height="45" Width="444" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
